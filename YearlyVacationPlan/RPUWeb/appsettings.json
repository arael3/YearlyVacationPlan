{
  "ApplicationInfo": {
    "Domain": "your-domain.net"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "logs/log.txt"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=dbName;Port=5432;User Id=postgres;Password=yourPass"
  },
  "Authentication": {
    "Google": {
      "ClientId": "your-google-oauth-client-id",
      "project_id": "your-google-project-id",
      "auth_uri": "https://accounts.google.com/o/oauth2/auth",
      "token_uri": "https://oauth2.googleapis.com/token",
      "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
      "ClientSecret": "your-google-oauth-client-secret",
      "redirect_uris": [ "https://your.domain.net/signin-google" ]
    }
  },
  "GoogleReCaptcha": {
    "SiteKey": "your-google-SiteKey",
    "SecretKey": "your-google-SecretKey"
  },
  "Smtp": {
    "Host": "smtp.your-domain.net",
    "Port": 587,
    "Username": "admin@your-domain.net",
    "Password": "your#password",
    "From": "admin@your-domain.net"
  }
}